{
"name": "Random Country Information Extension",
 
"version": "0.0.1",
  
"manifest_version": 2,
  
"description": "Get random country information when opening a new tab!",
  
"homepage_url": "https://codesparta.com",
  
"icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
 
"default_locale": "en",
  
"chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
"permissions": ["https://localhost/REST/api.php/city", 
"https://localhost/REST/api.php/country"]
}